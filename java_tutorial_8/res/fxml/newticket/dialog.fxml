<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="gridP_main_container" alignment="center" hgap="10.0" minHeight="160.0" minWidth="330.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.daniel.moune.java.ebusreservation.newticket.Controller">
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" minWidth="190.0" prefWidth="190.0" />
        <ColumnConstraints halignment="CENTER" minWidth="200.0" prefWidth="200.0" />
    </columnConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
   <children>
       <Button fx:id="btn_cancel" cancelButton="true" mnemonicParsing="false" onMouseClicked="#closeNewTicketDialog" onMouseEntered="#displayCancelBtnToolTip" text="Cancel" GridPane.columnIndex="1" GridPane.rowIndex="4" />
       <Button fx:id="btn_ok" defaultButton="true" mnemonicParsing="false" onMouseClicked="#createNewTicket" onMouseEntered="#displayOkBtnToolTip" prefWidth="63.0" text="Ok" GridPane.rowIndex="4" />
       <Label text="Ticket's date" />
       <Label text="Ticket's Type" GridPane.rowIndex="1" />
       <Label text="Customer's name" GridPane.rowIndex="2" />
       <Label layoutX="38.0" layoutY="107.0" text="Customer's phone number" GridPane.rowIndex="3" />
       <DatePicker fx:id="dateP_ticket_date" onMouseEntered="#displayDpTicketDateToolTip" GridPane.columnIndex="1" />
       <ComboBox fx:id="comboB_ticket_type" editable="true" onMouseEntered="#displayCbTicketTypeToolTip" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
       <TextField fx:id="textF_customer_name" onMouseEntered="#displayTfCustomerNameToolTip" GridPane.columnIndex="1" GridPane.rowIndex="2" />
       <TextField fx:id="textF_customer_phone_number" layoutX="180.0" layoutY="102.0" onMouseEntered="#displayTfCustomerPhoneToolTip" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Button mnemonicParsing="false" text="Clean" GridPane.columnIndex="1" GridPane.rowIndex="5" />
   </children>
</GridPane>
